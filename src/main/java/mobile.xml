<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="2" name="New Browsers" parallel="tests">

    <test name="Android1">
        <parameter name="browser" value="Chrome"/>
        <parameter name="platformVersion" value="11"/>
        <parameter name="platform" value="Android"/>
        <parameter name="deviceName" value="Galaxy S20"/>
        <classes>
            <class name="demoMobile"/>
        </classes>
    </test>
    <test name="iOS13">
        <parameter name="browser" value="safari"/>
        <parameter name="platformVersion" value="15.0"/>
        <parameter name="platform" value="iOS"/>
        <parameter name="deviceName" value="iPad 10.2 (2019)"/>
        <classes>
            <class name="demoMobile"/>
        </classes>
    </test>






